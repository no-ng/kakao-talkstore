{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "preinstall": "node scripts/preinstall.js",
    "postinstall": "husky install",
    "test": "jest --env=jsdom"
